{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 2],
        "quotes": ["error", "single"],
        "no-unused-vars": ["error", { "vars": "all" }] // thêm vào dòng này
      }
}
